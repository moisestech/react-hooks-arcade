{
  "name": "react-hooks-arcade",
  "version": "1.0.0",
  "description": "React Hooks Arcade - showcasing a variety of application concepts through interactivity",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "NODE_ENV='production' webpack"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/syntax-dynamic-import",
      "@babel/plugin-transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moisestech/react-hooks-arcade.git"
  },
  "keywords": [
    "ReactJS",
    "React Hooks",
    "React Arcade",
    "useState",
    "useEffect",
    "useCallback",
    "ES8"
  ],
  "author": "Moises Sanabria",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moisestech/react-hooks-arcade/issues"
  },
  "homepage": "https://github.com/moisestech/react-hooks-arcade#readme",
  "dependencies": {
    "marked": "^1.1.1",
    "path": "^0.12.7",
    "prop-types": "15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^3.11.0",
    "react-infinite-scroll-component": "^5.1.0",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.1.0",
    "css-loader": "^4.2.2",
    "dotenv": "^8.2.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "process": "^0.11.10",
    "react-transition-group": "^4.4.1",
    "style-loader": "^1.2.1",
    "webpack": "^5.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
